{
  "name": "@hetero/shared",
  "version": "0.0.1",
  "description": "Shared library for heterodoc project",
  "type": "module",
  "main": "./dist/index.js",
  "exports": {
    ".": {
      "dev": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./*": "./*"
  },
  "types": "./dist/index.d.ts",
  "scripts": {
    "dev": "cross-env NODE_ENV=development && tsup --watch",
    "build": "cross-env NODE_ENV=production && tsup"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "tsup": "^6.7.0",
    "typescript": "^5.0.3"
  },
  "keywords": [
    "Utilities"
  ],
  "author": "ShenQingchuan",
  "license": "MIT"
}
